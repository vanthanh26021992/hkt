<div class="row">
	<form action="">
		<fieldset>
			<legend><%=res(moduleName)%></legend>
			<%
				for (var i = 0; i < listMenuItem.length; i++) {
			%>
			<div class="menuCol col-xs-6 col-md-4">
				<div moduleId="<%=module%>/<%=listMenuItem[i]%>"
					class="onClickMenuItemModule onClickMenuItem btn">
					<img alt="" moduleId="<%=module%>/<%=listMenuItem[i]%>"
						class="onClickMenuItem" src="images/<%=listImages[i]%>"> 
						<a class="onClickMenuItem btn " moduleId="<%=module%>/<%=listMenuItem[i]%>" href="#"> <%=res(listMenuItem[i])%>
					</a>
				</div>
			</div>
			<%
				}
			%>
		</fieldset>
	</form>



		<fieldset>
			<legend><%=moduleUnderName%></legend>
			<%
				for (var i = 0; i < listUnderMenuItems.length; i++) {
			%>
			<div class="menuCol col-xs-6 col-md-4">
				<div moduleId="<%=module%>/<%=listUnderMenuItems[i]%>"
					class="onClickMenuItemModule onClickMenuItem btn">
					<img alt="" moduleId="<%=module%>/<%=listUnderMenuItems[i]%>"
						class="onClickMenuItem" src="images/<%=listUnderImages[i]%>"> 
						<a class="onClickMenuItem btn " moduleId="<%=module%>/<%=listUnderMenuItems[i]%>" href="#"> <%=res(listUnderMenuItems[i])%>
					</a>
				</div>
			</div>
			<%
				}
			%>
		</fieldset>
</div>
